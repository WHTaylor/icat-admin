{
  "private": true,
  "name": "icat-admin",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "e2e": "cypress run"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ],
    "rules": {
      "prefer-template": "off",
      "radix": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "_+"
        }
      ]
    }
  },
  "devDependencies": {
    "cypress": "^10.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "netlify-plugin-cypress": "^2.2.0",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "dayjs": "^1.11.3",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^4.1.0",
    "typescript": "^4.9.5"
  }
}
